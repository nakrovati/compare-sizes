    "dev": "vite",
    "dev:host": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "eslint --ext .ts,.vue src",
    "lint:fix": "eslint --ext .ts,.vue --fix src",
    "style": "stylelint **/*.{scss,vue}",
    "style:fix": "stylelint **/*.{scss,vue} --fix",
    "format": "prettier . --write",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{html,json,md,yml,yaml}": "prettier --write",
    "*.scss": "stylelint --fix",
    "*.vue": [
      "eslint --fix",
      "stylelint --fix"
    ],
    "*.{js,cjs,ts}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@vueuse/core": "^9.3.0",
    "pinia": "^2.0.22",
