    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "stylelint": "^14.13.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.6",
    "vitest": "^0.24.0",
    "vue-tsc": "^1.0.0"
